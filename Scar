local function DHLISF_fake_script() -- HomeButton.LocalScript 
	local script = Instance.new('LocalScript', HomeButton)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	script.Parent.MouseButton1Click:Connect(function(plr)
	
		script.Parent.Parent.Parent.HomeFrame.Visible = true
		script.Parent.Parent.Parent.MobileLockFrame.Visible = false
		script.Parent.Parent.Parent.PCLockFrame.Visible = false
		script.Parent.Parent.Parent.FunStuffFrame.Visible = false
		print("Recall Function_01")
	end)
	
end
