local function RCEZJVB_fake_script() -- ScarletCC.LocalScript 
	local script = Instance.new('LocalScript', ScarletCC)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local Players = game:GetService("Players")
	local Player = Players.LocalPlayer
	
	local Mouse = Player:GetMouse()
	local Gui = script.Parent.frame
	local Open = false
	
	function PressV(key) 
		if (key == "v") then 
			if (Open == false) then
				Gui.Visible = true
				Open = true
			elseif (Open == true) then
				Gui.Visible = false
				Open = false
			end
		end
	end
	
	Mouse.KeyDown:Connect(PressV) 
	print("Menu Toggle Key for Scarlet is V")
end
